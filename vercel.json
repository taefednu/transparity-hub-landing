{
  "buildCommand": "git lfs pull || true && pnpm install && pnpm run build",
  "outputDirectory": "build",
  "framework": "vite",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

